/* CSS Document */
@charset "utf-8";

ul, li{
	text-decoration: none;
	list-style: none;
}

.gender-box{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 20px 15px;
}
.gender-box>h1{
	width: 100%;
	display: block;
	padding: 15px 0;
	box-sizing: border-box;
	font-family: "微软雅黑";
	color: #000;
	text-align: center;
	line-height: 28px;
	border-bottom: #000 solid 1px;
	border-top:#000 solid 1px;
}
#gender{
	width: 100%;
	display: block;
	padding: 10px 0;
	box-sizing: border-box;
	overflow: hidden;
	border-bottom: #000 solid 1px;
}
#gender>li{
	display:block;
	width: 100%;
	box-sizing: border-box;
	padding:55px 30px;
	position: relative;
}

#gender h2{
	width: 100%;
	font-family: "微软雅黑";
	font-size: 20px;
}
#gender a{
	width: 100%;
	font-family: "微软雅黑";
	font-size: 16px;
	line-height: 20px;
}
#gender>li span{
	width: 16px;
	height: 16px;
	border-radius: 50%;
	display: inline-block;
}
#gender>li:nth-child(1){
	background: #fff url(../images/goddess.jpg) no-repeat;
	background-size: 100%;
	text-align: right;
	margin-bottom: 10px;
}
#gender>li:nth-child(2){
	background: #000 url(../images/man.jpg) no-repeat;
	background-size: 100% 100%;
}

#gender>li:nth-child(1) h2{color: #000;}
#gender>li:nth-child(1) a {color: #000;}
#gender>li:nth-child(2) h2{color: #fff;}
#gender>li:nth-child(2) a {color: #fff;}

#gender>li:nth-child(1) span{
	border: #000 solid 1px;
	background-size: 100%;
	position: absolute;
	bottom: 30px;
	right: 50px;
}
#gender>li:nth-child(2) span{
	border: #fff solid 1px;
	background-size: 100%;
	background-position: 0 -19px;
	position: absolute;
	bottom: 28px;
	left: 31px;
}
.select01{
   background: #000 url(../images/select.png) no-repeat;
}
.select02{
   background: #fff url(../images/select.png) no-repeat;
}

.gender-box .next{
	width: 100%;
	display: block;
	margin-bottom: 60px;
	text-align: center;
}
.gender-box .next button {
    width: 40%;
    height: 36px;
    border-image-source: initial;
    border-image-slice: initial;
    border-image-width: initial;
    border-image-outset: initial;
    border-image-repeat: initial;
    color: rgb(255, 255, 255);
    font-family: 微软雅黑;
    font-size: 18px;
    margin-top: 15px;
    background: rgb(0, 0, 0);
    border-width: initial;
    border-style: none;
    border-color: initial;
}

.gender-box .next>a{
	display: block;
	line-height: 36px;
	color:#000;
	font-family: "微软雅黑";
	font-size: 14px;
}


/*选择爱好*/
#hobby{
	width: 100%;
	display: block;
	box-sizing: border-box;
	padding: 30px 0;
	overflow: hidden;
	border-bottom: #000 solid 1px;
}
#hobby>li{
	width: 25%;
	display: inline-block;
	float: left;
	overflow: hidden;
}
#hobby>li:nth-child(n+5){
	margin-top: 20px;
}

#hobby>li>a{
	width: 100%;
	display: block;
	overflow: hidden;
	text-align: center;
}
#hobby>li  span{
	width: 40px;
	height: 41px;
	background: url(../images/interest.png) no-repeat;
	background-size: 100%;
	margin:0 auto;
	display: inline-block;
}
#hobby>li:nth-child(2) span{background-position: 0 -47px;}
#hobby>li:nth-child(3) span{background-position: 0 -94px;}
#hobby>li:nth-child(4) span{background-position: 0 -141px;}
#hobby>li:nth-child(5) span{background-position: 0 -189px;}
#hobby>li:nth-child(6) span{background-position: 0 -237px;}
#hobby>li:nth-child(7) span{background-position: 0 -284px;}
#hobby>li:nth-child(8) span{background-position: 0 -331px;}
#hobby>li:nth-child(9) span{background-position: 0 -378px;}
#hobby>li:nth-child(10) span{background-position: 0 -426px;}
#hobby>li:nth-child(11) span{background-position: 0 -475px;}

#hobby>li>a p{
	display: inline-block;
	color:#000;
	font-family: "微软雅黑";
	font-size: 12px;
	text-align: center;
	position: relative;
	padding-right: 20px;
	box-sizing: border-box;
	line-height: 20px;
}
#hobby>li  i{
  width: 16px;
  height: 16px;
  border-radius: 50%;
  display: inline-block;
  position: absolute;
  top: 2px;
  right: 1px;
}
#hobby>li  i.black{
  background:#000 url(../images/select.png) no-repeat;
  background-size: 100%;
  background-position: 0 -41px;
}

/*登录注册*/
.content{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 0 15px;
	box-sizing: border-box;
	background-color: #fff;
}
.content .form-title{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 10px 0;
	text-align: center;
	border-bottom:#DADADA solid 1px;
	box-sizing: border-box;
}
.content .form-title>h2{
	text-align: center;
	display:inline-block;
	color:#000;
	font-family: "微软雅黑";
	font-size: 16px;
	line-height: 40px;
	position: relative;
	padding-left: 50px;
}
.content .form-title img{
	width: 40px;
	height: 40px;
	display: inline-block;
	border-radius: 50%;
	position: absolute;
	top: 0;
	left: 0;
}

#enroll{
	width: 100%;
	display: block;
	overflow: hidden;
}
#enroll>li{
	width: 50%;
	display: inline-block;
	float: left;
	text-align: center;
	color:#515151;
	font-family: "微软雅黑";
	font-size: 16px;
	line-height: 40px;
	position: relative;
}
#enroll>li.roll{
	color: #989898;
}

#enroll>li span{
	width: 1px;
	height: 20px;
	display: inline-block;
	position: absolute;
	background: #515151;
	top: 10px;
	left: 0;
}

.log01, .log02{
	width: 100%;
	display: none;
	overflow: hidden;
}
.log01 ul, .log02 ul{
	width: 100%;
	display: block;
	overflow: hidden;
	margin-top: 10px;
}
.log01 ul>li, .log02 ul>li{
	width: 100%;
	height: 40px;
	display: block;
	position: relative;
	margin-bottom: 20px;
}
.log01 ul>li input, .log02 ul>li input{
	width: 100%;
	height: 40px;
	display: inline-block;
	line-height: 40px;
	padding-left: 5px;
	box-sizing: border-box;
	border: #DADADA solid 1px;
	color:#515151;
	font-family: "微软雅黑";
	font-size: 16px;
}

.log01 ul>li button, .log02 ul>li button{
	width: 50%;
	line-height: 36px;
	background-color: #000;
	margin-left: 25%;
	color: #fff;
	font-family: "微软雅黑";
	font-size: 16px;
	line-height: 36px;
	border:none;
	margin-top: 20px;
}
.log02 ul>li a{
	width: 110px;
	height: 40px;
	display: inline-block;
	position: absolute;
	top:0;
	right: 0;
	background-color: #000;
	color: #fff;
	font-family: "微软雅黑";
	font-size: 15px;
	text-align: center;
	line-height: 40px;
}
.log02 ul>li span, .log01 ul>li span{
	display: inline-block;
	position: absolute;
	bottom: -16px;
	left: 5px;
	color:red;
	font-family: "微软雅黑";
	font-size: 12px;
}

/*我的佣金*/
#between{
	width: 100%;
	display: block;
	overflow: hidden;
	background-color: #fff;
}
#between>li{
	width: 100%;
	height: 50px;
	display: block;
	overflow: hidden;
	line-height: 50px;
    border-bottom: #e3e2e0 solid 1px;
}
#between ol{
	width: 100%;
	display: block;
	overflow: hidden;
}
#between ol>li{
	width: 50%;
	display: inline-block;
	float: left;
	text-align: center;
	position: relative;
	color:#000;
	font-family: "微软雅黑";
	font-size: 16px;
}
#between ol>li i{
	font-size: 13px;
}
#between ol>li span{
	width: 1px;
	height: 50px;
	background-color: #e3e2e0;
	position: absolute;
	top: 0;
	left: 0;
}
#between>li:nth-child(2){
	padding: 0 15px;
	box-sizing: border-box;
	color:#000;
	font-family: "微软雅黑";
	font-size: 16px;
}
#between>li:nth-child(2) p{
	display: inline-block;
	float: right;
	color:#000;
	font-family: "微软雅黑";
	font-size: 16px;
}
#grade{
	width: 100%;
	display: block;
	overflow: hidden;
	height: 50px;
	background-color: #fff;
}
#grade>li{
	width: 33%;
	height: 50px;
	display: inline-block;
	float: left;
	text-align: center;
	color:#000;
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 20px;
	padding-top: 5px;
}
#grade>li.tabin{
	color: #949494;
}
#grade>li:nth-child(n+2){
   border-left: #e3e2e0 solid 1px;
}
.member1{
	width: 100%;
	display: none;
	overflow: hidden;
	background-color: #eee;
	margin-bottom: 60px;
}
.member1>ul{
	width: 100%;
	display: block;
	overflow: hidden;
	padding:10px 15px;
	border-bottom: #e3e2e0 solid 1px;
	box-sizing: border-box;
	margin-bottom: 5px;
	border-top: #e3e2e0 solid 1px;
	background-color: #fff;
	position: relative;
}
.member1>ul>li{
	display: inline-block;
	float: left;
	color:#000;
	font-family: "微软雅黑";
	font-size: 14px;
}
.member1>ul>li img{
	width: 50px;
	height: 50px;
	display: inline-block;
}
.member1 ol>li>a{
	color:#000;
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 17px;
}
.member1>ul>li:nth-child(1){
	margin-right: 15px;
}
.member1>ul>li:nth-child(3){
	text-align: center;
	float: right;
	height: 0;
}
.member1>ul>li:nth-child(4){
	width:100%;
	height: 30px;
	position: relative;
}

.member1>ul ol>li a.afr{
	display: inline-block;
	position: absolute;
	top:10px;
	right: 15px;
	font-size: 12px;
}
.member1>ul ol>li a.ayc{
	width:120px;
	display: inline-block;
	text-overflow:ellipsis;
	overflow: hidden;
	white-space:nowrap;
	font-weight: 600;
}

.member1>ul>li:nth-child(4) dl{
	width: 100%;
	height: 40px;
	font-size: 13px;
	line-height: 30px;
	margin-top: 5px;
	border-top: #e3e2e0 solid 1px;
}
.member1>ul>li:nth-child(4) dl dd{
	float: left;
}
.member1>ul dl dd:nth-child(1){
	width: 49%;
}
.member1>ul dl dd:nth-child(2){
	width: 32%;
	text-align: center;
}
.member1>ul dl dd:nth-child(3){
	width: 35%;
}

.member1>ul>li:nth-child(4) span{
	display: inline-block;
	position: absolute;
	top: 20px;
	left: 0;
	font-size: 12px;
}



/*提现页面*/
.withdraw-box{
	width: 100%;
	display: block;
	overflow: hidden;
}
#withdraw, .bank{
	width: 100%;
	overflow: hidden;
	background-color: #fff;
	display: block;
}
#withdraw>li, .bank>li{
	width: 100%;
	display: block;
	height: 40px;
	line-height: 40px;
	border-bottom: #e0e0e0 solid 1px;
	padding: 0 15px;
	color:#000;
	font-family: "微软雅黑";
	font-size: 14px;
	box-sizing: border-box;
}
.bank>li label{
	display: inline-block;
	float: left;
	height: 40px;
	line-height: 40px;
}
.withdraw-box h4{
	color:#000;
	font-family: "微软雅黑";
	font-size: 16px;
	width: 100%;
	line-height: 40px;
	border-bottom: #e0e0e0 solid 1px;
	padding: 0 15px;
	overflow: hidden;
	font-weight: 600;
}
.bank>li h4{
	font-size: 16px;
}
#withdraw>li>label{
	width: 18px;
	height: 18px;
	border-radius: 50%;
	display: inline-block;
	margin-right: 5px;
	position: relative;
	margin-top: 10px;
	border:solid 1px #000;
	float: left;
}
#withdraw>li>label input{
	width: 17px;
	height: 18px;
	display: inline-block;
	position: absolute;
	top:1px;
	left: 1px;
	opacity: 0;
}

.bank{
	width: 100%;
	display: none;
}
.bank select{
	width: 200px;
	height: 26px;
}
.bank>li:nth-child(4), .bank>li:nth-child(5){
	margin-bottom: 20px;
	position: relative;
}

.bank>li:nth-child(4) span, .bank>li:nth-child(5) span{
	position: absolute;
	left: 15px;
	bottom: -32px;
	font-size: 12px;
	color:#666;
}
.bank>li input{
	width: 200px;
	height: 30px;
	display: inline-block;
	float: left;
	line-height: 30px;
	padding-left: 5px;
	font-family: "微软雅黑";
	font-size: 14px;
	border:none;
	margin-top: 5px;
}
.bank>li button{
	width: 40%;
	height: 34px;
	margin-left: 30%;
	border:none;
	background-color: #000;
	color:#fff;
	font-family: "微软雅黑";
	font-size: 16px;
	line-height: 30px;
}
.bank>li:nth-child(7){
	border:none;
	margin-top: 10px;
}
.bank>li a{
	color: #000;
}
.bank>li:nth-child(8){
	border-top: #e0e0e0 solid 1px;
	margin-top: 30px;
	padding: 0;
}
.bank>li:nth-child(8) a{
	width: 100%;
	height: 40px;
	padding: 0 15px;
	box-sizing: border-box;
	display: inline-block;
	position: relative;
}
/*提现记录*/
.record-box{
	width: 100%;
	display: block;
	overflow: hidden;
	background-color: #eaeaea;
	margin-top: -1px;
}
.record-box ul{
	width: 100%;
	display: block;
	overflow: hidden;
	margin-bottom: 5px;
	border-top: #e0e0e0 solid 1px;
	background-color: #fff;
}
.record-box ul>li{
	width: 100%;
	display: block;
	border-bottom: #e0e0e0 solid 1px;
	overflow: hidden;
	color:#666;
	font-family: "微软雅黑";
	font-size: 13px;
	box-sizing: border-box;
}
.record-box ul>li span{
	display: inline-block;
	float: left;
}
.record-box ul>li:nth-child(1) span{
	color: #000;
	font-size: 12px;
	width:100%;
}
.record-box ul>li:nth-child(1) a{
	color: #000;
	font-size: 14px;
}
.record-box ul>li:nth-child(1) {
  line-height: 18px;
  padding: 10px 10px;
}
.record-box ol{
	width: 100%;
	overflow: hidden;
}
.record-box ol>li{
	display: inline-block;
	float: left;
	height: 40px;
	line-height: 18px;
	box-sizing: border-box;
	padding-top: 11px;
}
.record-box ol>li:nth-child(1){
	width:20%;
	border-right: #e0e0e0 solid 1px;
}
.record-box ol>li:nth-child(2){
	padding-left: 10px;
	box-sizing: border-box;
	position: relative;
	width:79%;
}
.record-box ol>li:nth-child(2) span{
	display: inline-block;
	width:90px;
	height: 40px;
	background: #000;
	color:#fff;
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 40px;
	text-align: center;
	position: absolute;
	top: 0;
	right:-13px;
}
.record-box .record-ol1, .record-box .record-ol2, .record-box .record-ol3{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 0 10px 0 10px;
	box-sizing: border-box;
	position: relative;
}
.record-box .record-ol2, .record-box .record-ol3{
	height: 40px;
}
.record-box .record-ol1 li{height: 60px;}
.record-box .record-ol1 li:nth-child(1){
	line-height: 40px;
}
.record-box .record-ol1 li:nth-child(2) a{
	line-height: 20px;
}


/*分享二维码页面*/
.two-dimension{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 50px;
	box-sizing: border-box;
	background-color: #1a1a1a;
}
.two-dimension>ul img{
	width: 100%;
}
.two-dimension>ul{
	width: 100%;
	display: block;
	overflow: hidden;
}
.two-dimension>ul>li{
	width: 33.3%;
	display: inline-block;
	float: left;
	text-align: center;
}
.two-dimension>ul>li:nth-child(3){
	float: right;
}
.two-dimension>ul>li span{
	display: inline-block;
	font-size: 40px;
	margin-top: 25%;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg); /* Internet Explorer */
	-moz-transform:rotate(45deg); /* Firefox */
	-webkit-transform:rotate(45deg); /* Safari 和 Chrome */
	-o-transform:rotate(45deg); /* Opera */
	color: #fff;
}

.two-dimension .dimension{
	width: 100%;
	display: block;
	overflow: hidden;
	position: relative;
	margin-top: 40px;
	padding: 15px 0;
	box-sizing: border-box;
}
.two-dimension .dimension p{
	color: #fff;
	font-family: "微软雅黑";
	font-size: 14px;
	text-align: center;
	opacity: .8;
	line-height: 20px;
}
.two-dimension .dimension span{
	width: 30%;
	height: 1px;
	display: inline-block;
	background-color: #fff;
	opacity: .8;
}
.two-dimension .dimension span.dime01{
	position: absolute;
	top: 0;
	left: 35%;
}
.two-dimension .dimension span.dime02{
	position: absolute;
	bottom: 0;
	left: 35%;
}
.share-ewm{
	width: 100%;
	display: block;
	overflow: hidden;
	text-align: center;
}
.share-ewm img{
	width: 40%;
	display: inline-block;
	margin: 20px 30%;
}
.share-ewm a{
	width: 100%;
	display: block;
	color: #000;
	font-family: "微软雅黑";
	font-size: 16px;
}


/*服务协议页面*/
.content{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 20px 10px 30px 10px;
	box-sizing: border-box;
}
.content>h2{
	width: 100%;
	display: block;
	text-align: center;
	line-height: 40px;
	color: #000;
	font-family: "微软雅黑";
	font-size: 18px;
}
.content>p{
	color: #3E3E3E;
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 18px;
	text-indent:2em;
}
.content>ul{
	width: 100%;
	display: block;
	overflow: hidden;
}
.content>ul>li{
	width: 100%;
	display: block;
	overflow: hidden;
}
.content>ul>li h2{
	color: #000;
	font-family: "微软雅黑";
	font-size: 16px;
	line-height: 28px;
}
.content>ul>li p{
	text-indent:2em;
	color: #3E3E3E;
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 18px;
}
.content>ul>li p a{
	color: #000;
	font-family: "微软雅黑";
	font-size: 15px;
}
.content ol{
	width: 100%;
	display: block;
	overflow: hidden;
	box-sizing: border-box;
}
.content ol.padd-20{
	padding-left: 20px;
}
.content ol>li{
	width: 100%;
	color: #000;
	font-family: "微软雅黑";
	font-size: 14px;
	line-height: 18px;
}
.content ol>li h3{
	width: 100%;
	color: #000;
	font-family: "微软雅黑";
	font-size: 16px;
	line-height: 20px;
	text-indent:2em;
}
.content ol>li a{
	padding-left: 20px;
}

