@charset "utf-8";
/*首页签到功能*/
.l-pop {
	position: fixed;
	z-index: 9999;
	left: 50%;
	top: 50%;
	padding: 8px 10px;
	width: 120px;
	min-width: 120px;
	border-radius: 4px;
	background:#fff;
	box-shadow:0 1px 5px rgba(0,0,0,.1);
	-webkit-transform:translate(-50%, -50%);
	transform:translate(-50%, -50%);
}
.l-pop .loading-spinner {
	margin: 1px auto -5px;
	height: 65px;
	width: 65px;
	background-image: url(./img/loading.gif);
	background-size: 100%;
	background-repeat: no-repeat;
}
.l-pop .msg {
  font-size:15px;
  color:#ec652a;
  text-align:center;
}
.l-overlay {
	height: 100%;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 999;
}
.l-message {
	top:45%;
	left:0;
	width:100%;
	padding:0;
	text-align:center;
	background:none;
	-webkit-box-shadow:none;
	box-shadow:none;
	-webkit-transform:translate(0, -60%);
	transform:translate(0, -60%);
}
.l-message .msg {
	display: inline-block;
	color:#ec652a;
	max-width:240px;
	font-size:16px;
	padding:10px 22px;
	background:#fff;
	-webkit-box-shadow:0 1px 5px rgba(0,0,0,.1);
	box-shadow:0 1px 5px rgba(0,0,0,.1);
	-webkit-border-radius:4px;
	border-radius:4px;
}
i.iconfont.ok, i.iconfont.no {
	position:relative;
	top:-1px;
	margin-right:4px;
}
i.iconfont.ok::before {
	content:'\e645';
}
i.iconfont.no::before {
	content:'\e646';
}

#menu .am_active {
  display: block;
}
.menu_list {
	display: none;
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	top: 45px;
	z-index: 100000;
}
.index_menu_list {
	top:0;
}
.menu_bg {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background: rgba(0,0,0,.4);
}
.menu_am_active {
  -webkit-transform: translateX(0);
  -ms-transform: translateX(0);
  transform: translateX(0);
}
.nav_menu {
  width: 40%;
  min-width: 180px;
  background: #fff;
  height: 100%;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  -webkit-transition: -webkit-transform .3s ease-in-out;
  transition: transform .3s ease-in-out;
  -webkit-transform: translateX(-100%);
  transform: translateX(-100%);
  z-index: 100001;
  background: #fff;
}
.nav_menu ul {
	list-style:none;
	padding:0;
	margin-bottom:60px;
}
.nav_menu ul li i {
	display: inline-block;
	float: left;
	width: 50px;
	height: 50px;
	line-height:55px;
	text-align:center;
}
.nav_menu ul li i::before {
	font-family:'iconfont';
	content:'\e616';
	font-size:32px;
	color:#ec652a;
	font-style:normal;
}
.nav_menu ul li:nth-child(2) i::before {
	content:'\e61b';
}
.nav_menu ul li:nth-child(3) i::before {
	content:'\e617';
}
.nav_menu ul li:nth-child(4) i::before {
	content:'\e615';
}
.nav_menu ul li:nth-child(5) i::before {
	content:'\e618';
}
.nav_menu ul li:nth-child(6) i::before {
	content:'\e619';
}
.nav_menu ul li:nth-child(7) i::before {
	content:'\e61c';
}
.nav_menu .nav-item {
	height: 60px;
	line-height: 60px;
	margin-left: 50px;
	background-color: #f3f3f3;
	color: #666666;
	font-weight: 600;
	font-size: 16px;
	padding-left: 20px;
	border-bottom: 1px solid #e9e9e9;
}
.nav_menu a:hover, .nav_menu a:focus {
  text-decoration: none;
}
.menu_am_active {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}
.nav_menu ul li:nth-child(2) i {
	background-size:85% auto;
	background-position:4px -35px;
}
.nav_menu ul li:nth-child(3) i {
	background-position:1px -85px;
}
.nav_menu ul li:nth-child(4) i {
	background-size:85% auto;
	background-position:3px -117px;
}
.nav_menu ul li:nth-child(5) i {
	background-position:1px -309px;
}
.nav_menu ul li:nth-child(6) i {
	background-position:1px -219px;
}
.nav_menu ul li:nth-child(7) i {
	background-position:1px -266px;
}
.nav_menu ul li:nth-child(8) i {
	background-position:1px -172px;
}

.panel-body::before {
	display:none;
}
