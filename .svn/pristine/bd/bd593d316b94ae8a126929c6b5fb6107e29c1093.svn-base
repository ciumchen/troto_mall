/* 语音充值 */
.see-order-addr {
	position:relative;
}
.see-order-addr ul {
	position:relative;
	color:#7a7a7a;
	padding:5px 5px 5px 40px;
	line-height:1.6;
}
.see-order-addr ul::before {
	position: absolute;
	left:10px;
	top:50%;
	font-family: 'iconfont';
	font-size:20px;
	content:'\e60d';
	-webkit-transform:translateY(-50%);
	transform:translateY(-50%);
}
.see-order-switch {
	position:absolute;
	top:10px;
	right:15px;
	width:45px;
	height:21px;
	background:url(./img/icon-switch1.png) no-repeat;
	background-size:100% auto;
}
.see-order-switch span, .see-order-switch i {
	position:absolute;
	width:100%;
	height:100%;
	opacity:0;
	background:url(./img/icon-switch1.png) no-repeat 0 -25px;
	background-size:100% auto;
	-webkit-transition:all .2s ease-in-out;
	transition:all .2s ease-in-out;
}
.see-order-switch i {
	left:2px;
	top:1px;
	width:100%;
	height:18px;
	opacity:1;
	background-size:42px auto;
	background-position:0 -46px;
}
.see-order-switch.on span {
	opacity:1;
}
.see-order-switch.on i {
	left:26px;
}

.voice-order-footer {
