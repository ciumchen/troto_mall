}
.community-nav li.on {
	color:#fff;
	background:#ec652a;
	border-top:1px solid #ec652a;
}
.community-nav i { 
	display:block;
	font-size:24px;
}
.community-nav i::before {
	content:'\e649';
}
.community-nav li:nth-child(2) i::before {
	content:'\e628';
}
.community-nav li:nth-child(3) i::before {
	content:'\e648';
}
.community-nav li:nth-child(4) i::before {
	content:'\e647';
}
.community-nav li:nth-child(5) i::before {
	content:'\e629';
}
.community-list {
	width:95%;
	margin:0 auto 5px;
}
.community-list.on {
	margin-top:80px;
}
.community-list .index-class-list {
	margin-bottom:8px;
}
.community-list li i {
	display:none;
	position:absolute;
	right:-4px;
	top:-4px;
	color:#fff;
	font-style:normal;
	font-size: 12px;
    line-height: 10px;
    padding: 2px 3px 2px;
    background: #ec652a;
    border-radius: 20px;
}
.community-list li.on {
	-webkit-animation:Buttons .2s ease-in-out;
	animation:Buttons .2s ease-in-out;
}
.community-price {
	position: absolute;
    bottom: 40%;
    right: 5px;
    font-size: 12px;
    padding: 3px 3px 2px;
    background: #ec652a;
    border-radius: 10px;
    color: #FFF;
}
.community-info {
	height:44px;
	padding:6px 4px 4px;
}
.community-list .community-info h3 {
	display:none;
	margin:0;
	white-space:normal;
	overflow: hidden;
    text-overflow: clip;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.community-info span {
	position:relative;
	display:none;
	width:30px;
	height:30px;
	margin:0 auto;
	background:#ec652a;
	-webkit-border-radius:50%;
	border-radius:50%;
}
.community-info span:active, .community-info span:hover {
	background:#df5c23;
}
.community-info span::after {
	position:absolute;
	top:50%;
	left:50%;
	width:16px;
	height:2px;
	content:'';
	background:#fff;
	-webkit-transform:translate(-50%, -50%);
	transform:translate(-50%, -50%);
}
.community-footer {
	position:fixed;
	left:0;
	bottom:0;
	width:100%;
	height:60px;
	padding:10px 12px;
	background:#fff;
	border-top:1px solid #e3e3e3;
}
.community-desc {
	float:right;
	line-height:40px;
	margin-right:10px;
	color:#ec652a;
}
.community-clearing-btn:link, .community-clearing-btn:visited {
	float:right;
	color:#fff;
	font-size:15px;
	letter-spacing:2px;
	padding:0 20px;
	line-height:40px;
	background:#ec652a;
}
.community-clearing-btn:active, .community-clearing-btn:hover {
	background:#df5c23;
}
.community-total p {
	margin-bottom:0;
	color:#999;
	font-size:12px;
}
.community-total .community-total-price {
	font-size:16px;
	color:#333;
	line-height:40px;		/*临时*/
}
.community-total p span {
	color:#ec652a;
}
.community-logo {
	width:80px;
	margin:0 auto 70px;
}
.community-logo img {
	display:block;
	width:100%;
}
@-webkit-keyframes Buttons {
	0%, 100% {-webkit-transform:scale(1);}
	50% {-webkit-transform:scale(.92);}
}
@keyframes Buttons {
	0%, 100% {transform:scale(1);}
	50% {transform:scale(.92);}
}

