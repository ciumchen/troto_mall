	box-shadow:none;
	background:#df5c23;
}
.voice-order-addr {
	position:relative;
	width:95%;
	margin:10px auto 12px;
  	box-shadow: 0 1px 2px rgba(0,0,0,.2);
}
.voice-order-addr img {
	display:block;
	width:100%;
}
.voice-order-txt {
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:100%;
	padding:10px;
}
.voice-order-txt ul {
	position:relative;
	top:50%;
	-webkit-transform:translateY(-50%);
}
.voice-order-txt li {
	position:relative;
	color:#7a7a7a;
	padding-left:22px;
	line-height:1.2;
	margin-bottom:5px;
}
.voice-order-txt li:last-child {
	margin-bottom:0;
}
