/*产品列表*/
.product-list {
	list-style:none;
	margin-bottom:57px;
}
.product-list li {
	position: relative;
	padding: 0;
	border-bottom: 3px solid #ec652a;
	-webkit-box-shadow: none;
	box-shadow: none;
}
.product-list li a {
	position:relative;
	display:block;
}
.brand-info {
	position: absolute;
	top: 0;
	left: 0;
	width: 50%;
	height: 100%;
