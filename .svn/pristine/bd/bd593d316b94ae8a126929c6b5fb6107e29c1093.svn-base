/* 拆开红包 */
.get-receive-top {
    height:80%;
}
.get-red-packet {
    position: absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    overflow: hidden;
    background:#fff;
    -webkit-perspective: 1000;
    perspective: 1000;
    -ms-transform: perspective(1000px);
    -moz-transform: perspective(1000px);
    -moz-transform-style: preserve-3d;
    -ms-transform-style: preserve-3d;
}
.red-packet {
    position: absolute;
    top:5%;
    left:50%;
    width:240px;
    height:350px;
    margin-left:-120px;
    -webkit-perspective: 1000;
    perspective: 1000;
    -webkit-transform-style: preserve-3d;
    transform-style: preserve-3d;   
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
    -webkit-transition: 0.6s;
    transition: 0.6s;
    -webkit-transition: 0.6s;
    -webkit-transform-style: preserve-3d;
    -moz-transform: perspective(1000px);
    -moz-transform-style: preserve-3d;
    position: relative;
}
.front, .back {
    position: absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background:url(img/red-packet-bg.png) no-repeat;
    background-size:100% auto;
    -webkit-transform-style:preserv-3d;
    transform-style:preserv-3d;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
    -webkit-transition: 0.6s;
    transition: 0.6s;

}
.front {
    z-index:2;
    -webkit-transform:rotateY(0deg);
    transform:rotateY(0deg);
}
.back {
    background-image:url(img/red-packet-bg-back.png);
    -webkit-transform:rotateY(-180deg);
    transform:rotateY(-180deg);
}
.red-packet.on .front {
    -webkit-transform: rotateY(180deg);
    transform: rotateY(180deg);
}
.red-packet.on .back {
    -webkit-transform: rotateY(0deg);
    transform: rotateY(0deg);
}
