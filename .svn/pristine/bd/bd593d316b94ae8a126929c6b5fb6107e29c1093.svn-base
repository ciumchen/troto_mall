/* ***************** 一小时送达页面 **************** */
.community-banner {
	margin-bottom:8px;
}
.community-search {
	height:36px;
}
.community-search p {
	margin-left:15px;
}
.community-nav {
	position:relative;
	margin-bottom:10px;
}
.community-nav.on {
	position:fixed;
	top:0;
	left:0;
	width:100%;
	z-index:999;
}
.community-nav ul {
	position:relative;
	width:100%;
	overflow:hidden;
	background:#fff;
}
.community-nav li {
	float:left;
	width:20%;
	height:100%;
	color:#999;
	padding:5px 0 8px;
	font-size:13px;
	text-align:center;
	border-top:1px solid #dedede;
	-webkit-box-sizing:border-box;
