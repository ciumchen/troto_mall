	background:rgba(0,0,0,.6);
	background-size:auto 100%;
}
.brand-info h2 {
  font-size: 21px;
  font-weight: normal;
  padding-left: 12px;
  line-height: 1.1;
  width: 95%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  border-left: 4px solid #ec652a;
  margin: 9% 0 6%;
}
.brand-info p {
  font-size: 13px;
  margin-left: 16px;
  line-height: 1.4;
}
.brand-btn {
  position: absolute;
  top: 50%;
  right: 20px;
  color: #fff;
  font-size: 13px;
  padding: 6px 5px;
  line-height:1;
  overflow: hidden;
  background: #FF5E17 url(./img/iconbg.png) repeat-x;
  background-size: auto 100%;
  -webkit-border-radius: 2px;
  border-radius: 2px;
}
.brand-info p i {
	display:none;
  position: relative;
  top: -1px;
  /*display: inline-block;*/
  width: 12px;
  height: 14px;
  margin-left: 6px;
  font-style:normal;
}
.brand-info p span {
	color:#ec652a;
	font-size:16px;
	font-weight:bold;
}
.brand-info p i::before {
	display:block;
	content:'\e600';
	line-height:1;
	color:#ec652a;
	font-family:'iconfont';
}
.product-list li:nth-child(even) .brand-info,
.search-brand-list li:nth-child(even) .brand-info {
	left:auto;
	right:0;
}
.product-list li:nth-child(even) .brand-info h2,
.search-brand-list li:nth-child(even) .brand-info h2 {
	position: absolute;
	left: 0;
	bottom: -2px;
	width: 100%;
	padding: 0 12px 0 15%;
