	border:none;
	border-right:4px solid #ec652a;
}
.product-list li:nth-child(even) .brand-info p,
.search-brand-list li:nth-child(even) .brand-info p {
	margin: 8% 0 0 15%;
}
.product-list li:nth-child(even) .brand-btn,
.search-brand-list li:nth-child(even) .brand-btn {
	right:auto;
	left:20px
}
