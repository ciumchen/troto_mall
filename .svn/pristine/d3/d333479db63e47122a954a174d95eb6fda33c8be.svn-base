/* CSS Document */
@charset "utf-8";
.search {
	width: 100%;
	height: 60px;
	overflow: hidden;
	position: absolute;
	left: 0;
	bottom: -10px;
	z-index: 8888;
}
.search-border{
	width: 100%;
	height: 50px;
	overflow: hidden;
	box-sizing: border-box;
	text-align: center;
}
.search-border form{
	width: 70%;
	height: 50px;
	display: block;
	margin-left: 15%;
	overflow: hidden;
	position: relative;
}
.search-border input{
	width: 100%;
	height: 30px;
	border:#8e8e8e solid 1px;
	border-radius: 5px;
	background: #fff;
	font-size: 16px;
	padding-left: 5px;
	outline:none;
	margin-top: 9px;
	box-sizing: border-box;
	padding-right: 40px;
}
.search-border form span{
	width: 40px;
	height: 30px;
	display: inline-block;
	position: absolute;
	top: 9px;
	right: 1px;
	font-size: 20px;
	padding-top: 5px;
}
input::-ms-input-placeholder{text-align: center;}
input::-webkit-input-placeholder{text-align: center;}

.search-border h4{
	display: inline-block;
	height: 36px;
	margin: 0 auto;
	line-height: 20px;
	font-family: "微软雅黑";
	margin-top: 13px;
	font-size: 16px;
	position: relative;
	color: #828282;
}
.search-border h4 span{
	display: inline-block;
	position: absolute;
	top: 2px;
	left: -10px;
}

#no-msg{
	width: 100%;
	display: block;
	line-height: 25px;
	padding-left: 20%;
	color:#b9b9b9;
}

/*æ¯æ—¥é™æ—¶æŠ¢è´­*/
.panic-box{
	width: 100%;
	overflow: hidden;
	display: block;
	position: relative;
}
.panic-box a{
	width: 100%;
	display: block;
	overflow: hidden;
	box-sizing: border-box;
	font-size: 12px;
}
.panic-box ul {
	width: 100%;
	height: 36px;
	background-color: #fff;
	padding: 0 20px;
	box-sizing: border-box;
	margin-top: -6px;
}
.panic-box ul li{
  height: 36px;
  line-height: 40px;
  display: inline-block;
  float: left;
  position: relative;
  font-family: "微软雅黑";
  padding-left: 20px;
}
.panic-box ul li:nth-child(1){
  width: 55%;
}
.panic-box ul li span{
	display: inline-block;
	position: absolute;
	top: 13px;
	left: 3px;
}

.panic-box ul li:nth-child(2){
	text-align: right;
	display: inline-block;
	float: right;
}

/*éš”ç¦»æ¡*/
#quarantine{
	width: 100%;
	height: 5px;
	overflow: hidden;
	display: block;
	border-top: #e1e1e1 solid 1px;
	border-bottom: #e1e1e1 solid 1px;
}

#quarantine2{
	width: 100%;
	overflow: hidden;
	display: block;
	border-top: #fff solid 4px;
	border-bottom: #fff solid 4px;
}

#quarantine2 span{
	width: 100%;
	height: 5px;
	overflow: hidden;
	display: block;
	border-top: #e1e1e1 solid 1px;
	border-bottom: #e1e1e1 solid 1px;
}

/*æ–°å“æŽ¨è*/
.panic-box p, .swiper-container p {
	display: inline-block;
	position: absolute;
	right: 10px;
	bottom: 5px;
	font-family: "微软雅黑";
	font-size: 12px;
	z-index: 8888;
}
.content-box{
	width: 100%;
	overflow: hidden;
	padding: 0 10px;
	margin-bottom: 60px;
}
.content-tit{
	width: 100%;
	overflow: hidden;
	padding: 5px 10px;
	background-color: #fff;
}
.tite, .tite2, .tite3, .tite4{
	width:100%;
	height: 1px; 
	background: #828282;
	display: block;
	margin: 15px 0;
	position: relative;
}
.tite span, .tite2 span{
  width: 36%;
  display: inline-block;
  background: #eee;
  position: absolute;
  left: 32%;
  top: -10px;
  font-family: "微软雅黑";
  font-size: 16px;
  text-align: center;
  color: #000;
}

.tite3 span{
  width: 48%;
  display: inline-block;
  background: #fff;
  position: absolute;
  left: 26%;
  top: -8px;
  font-family: "微软雅黑";
  font-size: 15px;
  text-align: center;
  color: #000;
}

.tite2 {
	margin-top: 25px;
	opacity: .5;
}
.tite2 span{
  font-size: 14px;
  top: -8px;
}

.streamer{
	width: 100%;
	height: 50px;
	margin-top: 5px;
	display: block;
	overflow: hidden;
	background: #444 url(../images/load.jpg);
	background-size: 100% 100%;
	text-align: center;
}
.streamer>span{
	display: inline-block;
	margin: 0 auto;
	line-height: 50px;
	font-family: "微软雅黑";
	font-size: 18px;
	color:#fff;
	font-weight: 600;
}


/*å•†å“èœå•*/
#blush {
	width: 100%;
	display: block;
	overflow: hidden;
	line-height:17px
}
#blush li{
	width: 20%;
	display: inline-block;
	text-align: center;
	float: left;
	font-family: "微软雅黑";
	font-size: 12px;
	padding: 2px 0;
}
#blush li.tabin{
	color: #fff;
	background: #000;
}

/*å•†å“ç›’å­*/
.comm01, .comm02, .comm03, .comm04, .comm05{
	width: 100%;
	margin-top: 13px;
	display: block;
	overflow: hidden;
	display: none;
}
.wares{
	width: 100%;
	overflow: hidden;
}
.wares>li{
  width: 48.5%;
  display: inline-block;
  float: left;
  background-color: #fff;
  margin-bottom: 10px;
}
.wares>li.li2{
	width: 49%;
}
.wares>li:nth-child(even){
	margin-left: 3%;
}
.wares>li.li2:nth-child(even){
	margin-left: 2%;
}

.wares .wares-ul{
	/*border:#e1e1e1 solid 1px;*/
	width: 100%;
	display: block;
	overflow: hidden;
	box-sizing: border-box;
	padding: 5px;
	color: #000;
	font-family: "微软雅黑";
}

.wares .wares-ul> h3{
    display: block;
    padding-left: 28px;
    height: 32px;
    position: relative;
    line-height: 1.3;
    color: #000;
    font-size: 12px;
    overflow: hidden;
    display: -webkit-box;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-family: "微软雅黑";
}
.wares .wares-ul>h3 span{
  width: 20px;
  height: 20px;
  display: inline-block;
 /* background: url(../images/btn.png);*/
  background-size: 100% 100%;
  position: absolute;
  top: 2px;
  left: 5px;
}
.wares .wares-ul>li p{
	font-size: 11px;
	line-height: 16px;
	font-family: "微软雅黑";
	color: #000;
}
.wares .wares-ul>h2{
	text-align: center;
	font-size: 13px;
	color: #000;
}
.wares .wares-ul>p{
	padding: 5px;
	font-size: 12px;
	box-sizing: border-box;
	color: #000;
}
.wares .wares-ul>p span{
	display: inline-block;
	float: right;
	font-size: 14px;
	margin-top: 2px;
}

/*æ´»åŠ¨é¡µé¢*/
.ridge{
 width: 100%;
 display: block;
 padding: 10px 15px;
 background-color: #fff;
 overflow: hidden;
}

.ridge ul{
	width: 100%;
	height: 16px;
	display: block;
	box-sizing: border-box;
	border-right: 1px solid #000;
	border-left: 1px solid #000;
}

.ridge ul li{
	width: 33.3333%;
	height: 16px;
	display: inline-block;
	float: left;
	text-align: center;
	font-family: "微软雅黑";
	color: #000;
	font-size: 13px;
	line-height: 16px;
	position: relative;
}
.ridge ul span{
	width: 1px;
	height: 16px;
	display: inline-block;
	background-color: #000;
	position: absolute;
	top: 0;
	left: 0;
}
.ridge ul li.ridge-li{
	color: #828282;
}

.movable-box01, .movable-box02, .movable-box03, .movable-box04{
	width: 100%;
	display: none;
	overflow: hidden;
}

/*ä¼˜æƒ åˆ¸*/
.movable-box04{
	padding: 0 8px;
	margin-bottom: 60px;
}
.movable-box04>ul{
	width: 100%;
	display: block;
	padding: 10px 0;
	box-sizing: border-box;
	overflow: hidden;
}
.movable-box04>ul>li{
	width: 100%;
	text-align: center;
	line-height: 20px;
}
.movable-box04>ul>li:nth-child(1){
	font-size: 16px;
}
.certificate{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 5px 10px 5px 0;
	box-sizing: border-box;
	position: relative;
	border:#dddcdc solid 1px;
	background-color: #fff;
	margin-bottom: 8px;
}
.certificate ul{
	width: 100%;
	overflow: hidden;
	display: inline-block;
}

.certificate img {
	width: 70%;
	float: left;
}
.certificate ul{
	width: 30%;
	float: right;
	text-align: right;
}
.certificate li h2{
  font-size: 17px;
  font-family: "微软雅黑";
  color: #c56a6b;
}
.certificate li{
  font-size: 12px;
  font-family: "微软雅黑";
  line-height: 17px;
}
.certificate ol{
	display: inline-block;
	position: absolute;
	right: 10px;
	bottom: 6px;
}
.certificate ol li:nth-child(1){
	color: #828282;
	display: inline-block;
	padding-left: 20px;
	box-sizing: border-box;
	line-height: 18px;
	border-bottom: #e4e4e4 solid 1px;
}
.certificate ol li:nth-child(2){
	color: #000;
	text-align: right;
}

.swiper-movable{
	width: 100%;
	display: block;
	overflow: hidden;
	box-sizing: border-box;
}
.certificate span{
	width: 5px;
	height: 100%;
	display: inline-block;
	position: absolute;
	top: 0;
	right: 0;
	background-color: #c56a6b;
}


.flexible{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 0 10px;
	box-sizing: border-box;
}
.flexible ul{
	width: 100%;
	display: block;
	overflow: hidden;
	box-sizing: border-box;
}
.flexible ul li{
	width: 48.5%;
	display: inline-block;
	float: left;
	box-sizing: border-box;
	overflow: hidden;
}
.flexible ul li:nth-child(2){
	margin-left: 3%;
}

/*äº§å“åˆ—è¡¨*/
.produce-box{
	width: 100%;
	height: auto;
	display: block;
	margin: 5px 10px 10px 10px;
	overflow-y: hidden;
	overflow-x: auto;
}
.produce-box>ul{
	width: 660px;
	height: 130px;
	display: block;
}
.produce-box>ul>li{
	width: 120px;
	height: 130px;
	display: inline-block;
	float: left;
	margin-right: 10px;
	overflow: hidden;
}
.produce-li{
	width: 100%;
	display: block;
	overflow: hidden;
}
.produce-li>li{
	width: 100%;
	display: block;
	text-align: center;
	box-sizing: border-box;
	overflow: hidden;
}
.produce-li>li>h3{
	font-size: 1.1em;
	color: #000;
	font-family: "微软雅黑";
}
.produce-li>li:nth-child(2){
	margin-top: -5px;
}

.produce-li>li>img{
	width: 80%;
	display: inline-block;
	margin-left: 10%;
}
.produce-li>li p, .produce-li>li a{
 color: #cd6d6e;
}
.produce-li>li s{
	color: #828282;
}
.quarantine{
	width: 100%;
