	border-top: #e1e1e1 solid 1px;
}

/*å€’è®¡æ—¶*/
.count-down{
	width: 100%;
	display: block;
	margin-top: 5px;
	padding-top: 10px;
	border-bottom: #e1e1e1 solid 1px;
	border-top: #e1e1e1 solid 1px;
}
.count-down ul{
	width: 100%;
	display: block;
	box-sizing: border-box;
	padding: 5px 15px;
	overflow: hidden;
	position: relative;
}
.count-down.padd{
	padding: 0;
	border-top: #fff solid 1px;
	margin-top: 0;
}

.count-down ul li{
	display: inline-block;
	box-sizing: border-box;
	font-size: 14px;
	font-family: "微软雅黑";
}
.count-down ul li:nth-child(2){
	width: 40%;
	text-align: center;
	position: absolute;
	top: 5px;
	left: 30%;
}
.count-down ul li:nth-child(3){
	float: right;
}
.count-bottom{
	width: 100%;
	height: 100px;
	display: block;
	overflow: hidden;
	padding: 0 15px;
}

.vouchsafe{
	width: 100%;
	display: block;
	overflow: hidden;
	border-top: #e1e1e1 solid 1px;
	background-color: #fff;
	margin-top: 8px;
}

/*åº•éƒ¨*/
footer{
	width: 100%;
	height: 50px;
	display: block;
	position: fixed;
	left:0;
	bottom:0;
	background-color: #383838;
	z-index: 99999;
}
footer ul{
	width: 100%;
	overflow: hidden;
	height: 50px;
}
footer ul li{
	width: 25%;
	height: 50px;
	float: left;
	font-size: 11px;
	line-height: 16px;
	color:#f2f2f2;
	font-family: "微软雅黑";
	text-align: center;
	box-sizing: border-box;
	padding-top: 8px;
}
#foot li.active{
	color: #8B8B8B;
}
footer ul li span{
	width: 20px;
	height: 20px;
	display: block;
	margin: 0 auto;
	font-size: 18px;
}

