/*è®¢å•ç¡®è®¤*/
.location-affirm{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 10px 15px 10px 80px;
	position: relative;
	background-color: #fff;
}
.location-affirm>span{
	display: inline-block;
	position: absolute;
	top: 20px;
	left: 20px;
	font-size: 40px;
}

.location-affirm .location{
  display: inline-block;
  width: 100%;
  float: right;
  border:none;
  padding:0;
}
.location-affirm .location>li{
	width: 100%;
}

.main-content h2{
	font-size: 18px;
	color:#000;
	font-family: "微软雅黑";
}
.main-content{
	background-color: #fff;
}

.payment{
	width: 100%;
	background-color: #fff;
	display: block;
	overflow: hidden;
	margin-bottom: 50px;
}
.payment>ul{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 10px 15px 5px 15px;
	box-sizing: border-box;
	border-bottom: #e3e2e0 solid 1px;
}
.payment>ul>li{
	width: 100%;
	display:inline-block;
	float: right;
	text-align: right;
	font-size: 14px;
	color:#020202;
	font-family: "微软雅黑";
	margin-bottom: 5px;
}
.payment>ul>li a{
	color:#020202;
}
.payment>ul>li span{
	color:#020202;
	font-size: 16px;
	margin-right: 5px;
}
.payment>ul>li:nth-child(3) a{
   padding: 5px 10px;
   display: inline-block;
   overflow: hidden;
   box-sizing: border-box;
   border:solid #000 1px;
}
#rule{
	width: 100%;
	display: block;
	overflow: hidden;
	position: relative;
	font-size: 14px;
	padding: 10px 25px 10px 40px;
	box-sizing: border-box;
	color:#020202;
	font-family: "微软雅黑";
	border-bottom: #e3e2e0 solid 1px;
}
#rule>span{
	top: 6px;
}
#rule>label{
	display: inline-block;
	width: 16px;
	height: 16px;
	border-radius: 50%;
	border: solid #000 1px;
	position: absolute;
	top:10px;
	left:15px;
}
#rule>label>input{
	width: 16px;
	height: 16px;
	display: inline-block;
}
.payment button{
	width: 30%;
	height: 40px;
	margin-left: 35%;
	background-color: #000;
	color:#fff;
	border:none;
	margin-top: 15px;
	font-size: 16px;
	font-family: "微软雅黑";
}

.try-out{
	width: 100%;
	display: block;
	padding: 10px 0 10px 110px;
	box-sizing: border-box;
	border-top: #e3e2e0 solid 1px;
	position: relative;
	margin-top: 40px;
}
.try-out>li:nth-child(1){
	width: 36px;
	height: 36px;
	display: inline-block;
	position: absolute;
	top:16px;
	left: 0;
}
.try-out>li:nth-child(1) span{
	width: 36px;
	height: 36px;
	display: inline-block;
	background: url(../images/pay.png) no-repeat;
	background-size: 100%;
	background-position: 0 -88px;
}

.try-out>li:nth-child(2){
	display: inline-block;
	text-align: right;
	position: absolute;
	top:16px;
	left: 40px;
	padding: 0 5px;
	box-sizing: border-box;
}
.try-out>li p{
	font-family: "微软雅黑";
	font-size: 12px;
	line-height: 16px;
}
.try-out>li p.omit{
	white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.try-out>li h1{
	font-family: "微软雅黑";
	font-size: 16px;
}

.try-out>li:nth-child(3){
	width: 79%;
	display: inline-block;
	text-align: left;
	float: left;
	overflow: hidden;
	padding: 5px 0 5px 5px;
	box-sizing: border-box;
	border-left: solid 1px #e3e2e0;
}
.try-out>li:nth-child(4){
	width: 20%;
	text-align: right;
	float: left;
	font-family: "微软雅黑";
	font-size: 12px;
	line-height: 28px;
}

.look-up{
	width: 100%;
	background-color: #fff;
	padding:13px 15px;
	margin-bottom: 5px;
	border-bottom: #d8d8d8 solid 1px;
}

.look-up a{
	width: 100%;
	display: block;
	position: relative;
}
.look-up span{
	display: inline-block;
	float: left;
	font-size: 40px;
	color:#000;
	position: absolute;
	top: -14px;
	left: 0;
}
.look-up h3{
	display: inline-block;
	margin-left: 48px;
	font-family: "微软雅黑";
	color: #000;
}

.tariff{
	width: 100%;
	display: block;
	border-bottom: #d8d8d8 solid 1px;
	margin-top:5px;
	margin-bottom: 60px;
	background-color: #fff;
}
.tariff>ul{
	width: 100%;
	display: block;
	overflow: hidden;
}
.tariff>ul>li{
	width: 100%;
	display: block;
	border-top: #d8d8d8 solid 1px;
	padding: 0 15px;
	line-height: 40px;
	font-family: "微软雅黑";
	color: #000;
	overflow: hidden;
	box-sizing: border-box;
}
.tariff>ul>li h2{
	font-size: 16px;
	display: inline-block;
	float: left;
}
.tariff>ul>li>a{
	display: inline-block;
	float: right;
}
.tariff>ul>li span{
	display: inline-block;
	width: 80px;
	text-align: center;
	line-height: 30px;
	border: #000 solid 1px;
	float: right;
	margin: 5px 0;
	background: #000;
    color: #fff;
	font-family: "微软雅黑";
    font-size: 16px;
}

.tariff ol{
	width: 100%;
	display: block;
	overflow: hidden;
}
.tariff ol>li{
	height: 36px;
}



/*商品分类页面*/
.category{
	width: 100%;
	height: 50px;
	overflow: auto;
	display: block;
	background-color:#fff;
	-moz-box-shadow: 0px 10px 5px #888888; /* 老的 Firefox */
	box-shadow: 0px 1px 3px #888888;
	padding: 0 40px 0 5px;
	box-sizing: border-box;
	position: relative;
}
.category>ul{
	width: 300px;
	display: inline-block;
	overflow: auto;
	overflow: hidden;
	float: left;
	margin-top: 15px;
}
.category>ul>li{
	width: 70px;
	display: inline-block;
	float: left;
	font-size: 13px;
	font-family: "微软雅黑";
	text-align: center;
	margin-right: 2px;
	color: #8a8a8a;
	line-height: 20px;
}
.category>ul>li.cate{
	color:#000;
	border:#000 solid 1px;
	border-radius: 3px;
}
#cate{
	width: 35px;
	height: 50px;
	display: inline-block;
