.onoffswitch-inner {width: 200%;margin-left: -100%;-webkit-transition: margin 0.2s ease-in;transition: margin 0.2s ease-in;}
.onoffswitch-switch {position: absolute;top: 0;right:40px;bottom: 0;width: 24px;height: 24px;margin:1px 2px;background: white;border-radius: 20px;-webkit-transition: right 0.2s ease-in, -webkit-box-shadow 0.2s ease-in;transition: right 0.2s ease-in, box-shadow 0.2s ease-in;}
.onoffswitch-inner:before, .onoffswitch-inner:after {float: left;padding: 0;width: 50%;height: 26px;line-height: 26px;color: white;-webkit-box-sizing: border-box;box-sizing: border-box;}
.onoffswitch-inner:after {content: "NO";padding-right: 10px;color: #fff;background-color: #ec652a;text-align: right;}
.onoffswitch-inner:before {content: "YES";padding-left: 10px;color: #fff;background-color: #ec652a;}
