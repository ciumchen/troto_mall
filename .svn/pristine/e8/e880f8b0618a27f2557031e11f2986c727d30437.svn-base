/*下单成功页面*/
.order-prompt-info {
	position:relative;
}
.order-prompt-info img {
	display:block;
	width:100%;
}
.order-prompt-txt {
	position:absolute;
	top:50%;
	left:0;
	width:100%;
	text-align:center;
	-webkit-transform:translateY(-50%);
}
.order-prompt-txt i {
	display:block;
	width:35px;
	height:45px;
	margin:2px auto;
	background:url(./img/icon-order-ok.png) no-repeat;
	background-size:100% auto;
}
.order-prompt-txt h3 {
	margin:0;
	font-size:17px;
	color:#ec652a;
	padding-bottom:5px;
	margin-bottom: 2px;
}
.order-prompt-txt p {
	margin:0 10%;
	color:#7a7a7a;
	line-height:25px;
	border-top:1px dashed #b5b5b5;
}
.order-ok-info {
	padding:0 12px;
	color:#4c4c4c;
	font-size:15px;
}
.order-ok-info li {
	padding:12px 0;
	border-bottom:1px solid #f2f0f0;
}
.order-ok-info li:last-child {
	border:none;
}
