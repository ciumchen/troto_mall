/* CSS Document */
@charset "utf-8";

ul, li, ol{
	text-decoration: none;
	list-style: none;
}

/* 活动页面 */
* {
	margin:0px;
	padding: 0;
}
.start-business{
	width: 100%;
	display: block;
	overflow: hidden;
}
.start-title{
	width: 100%;
	display: block;
	overflow: hidden;
	padding: 10px 15px;
}
.start-title>img{
	width: 30%;
}
.business-box{
	width: 100%;
	display: block;
	padding: 0 15px;
	box-sizing: border-box;
}
.business-box .start-tit{
	width: 100%;
	height: 50px;
	display: block;
	background-image: url(../images/trabecula.jpg);
	background-repeat: repeat-x;
	text-align: center;
}
.business-box .start-tit h1{
	display: inline-block;
	font-family: "微软雅黑";
	color: #fff;
	font-size: 20px;
	position: relative;
	line-height: 48px;
}
.business-box .start-tit h1:before{
	content:"";
	width: 5px;
	height: 5px;
	display: inline-block;
	background-color: #fff;
	border-radius: 50%;
	position: absolute;
	top: 22px;
	left: -15px;
}
.business-box .start-tit h1:after{
	content:"";
	width: 5px;
	height: 5px;
	display: inline-block;
	background-color: #fff;
	border-radius: 50%;
	position: absolute;
	top: 22px;
	right: -15px;
}
.business-box>ul{
	width: 100%;
	display: block;
	overflow: hidden;
	text-align: left;
	padding: 0 10px 25px 10px;
	box-sizing: border-box;
	border-left: #e6e6e6 solid 1px;
	border-bottom: #e6e6e6 solid 1px;
	border-right: #e6e6e6 solid 1px;
}
.business-box>ul>li{
	width: 100%;
	display: block;
	line-height: 22px;
	font-size: 16px;
	font-family: "微软雅黑";
	color:#000;
	margin-top: 5px;
}
.business-box>ul>li>a{
	display: inline-block;
	color: #c70067;
	border-bottom: #c70067 solid 1px;
	box-sizing: border-box;
	padding: 0 2px;
}
.business-ol{
	width: 100%;
	display: block;
	margin-top: 10px;
	box-sizing: border-box;
	padding: 0 10px;
}
.business-ol>ol{
	width: 100%;
	display: block;
	background-color: #c70067;
	box-sizing: border-box;
	text-align: center;
	padding: 20px 0;
	border-radius: 5px;
}
.business-ol>ol>li, .business-ol>ol>li a{
	color:#fff;
}
.business-ol>ol>li a{
	display: inline-block;
	color: #fff;
	border-bottom: #fff solid 1px;
	box-sizing: border-box;
	padding: 0 2px;
}
.business-ol>ol>li:nth-child(n+2){
	margin-top: 5px;
}
.business-box h2{
	font-size: 18px;
	line-height: 22px;
	color: #be0062;
}
















