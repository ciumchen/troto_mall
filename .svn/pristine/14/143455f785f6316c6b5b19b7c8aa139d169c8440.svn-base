body {background:#fff;}

@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_1444895175_8541305.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/font_1444895175_8541305.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/font_1444895175_8541305.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/font_1444895175_8541305.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/font_1444895175_8541305.svg#iconfont') format('svg'); /* iOS 4.1- */
}

@media only screen and (min-width:640px) {
	.class-search-form {
		left: 0
	}

	.class-search-form p {
		width: 485px;
		left: 28px;
		margin: 0 auto;
	}

	.class-con {
		width: 500px
	}
}

@media only screen and (max-height:480px) {
	.grab-red-list {
		background-size: 100% auto
	}

	.red-goto-btn {
		bottom: -50%
	}

	.red-list-money ul {
		height: 136px
	}
}

@media only screen and (min-width:375px) {
	.red-money-info {
		margin: 8% auto 22%
	}

	.red-list-money ul {
		height: 272px
	}
}
.list-load,.loading,.prompt-main {
	left: 50%;
	text-align: center
}
.list-load {
	display: none;
	position: absolute;
	bottom: -30px;
	width: 85px;
	color: #ccc;
	-webkit-transform: translateX(-60%);
	transform: translateX(-60%)
}

.list-load img {
	display: block;
	width: 100%
}
@charset "utf-8";

.brand-info p i::before,.index-search-button,.love-btn::before,.nav_menu ul li i::before,.rechange-main::before {
	font-family: iconfont
}


.index-search {
	position: relative;
	padding: 8px 10px
}

.index-search .local,.index-search-button {
	position: absolute;
	top: 8px;
	color: #fff;
	line-height: 32px;
	text-align: center
}

.index-search .local {
	left: 10px;
	width: 50px;
	font-size: 14px;
	background: #ec652a;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis
}

.index-search-txt {
	margin: 0 42px 0 60px
}

.index-search-txt input {
	-webkit-border-radius: 0;
	border-radius: 0;
	height: 32px;
	padding: 0 4px;
	color: #666;
	font-size: 14px;
	text-align: center;
	background: #ececec;
	-webkit-box-shadow: none;
	box-shadow: none
}

.index-search-txt input:focus {
	border-color: #ccc
}

.index-search-button {
	right: 10px;
	padding: 0;
	border: none;
	width: 32px;
	font-size: 20px;
	background: #ec652a
}

.search-form p {
    position: relative;
}
p {
    display: block;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
}
.logo-class {
	text-align: center;
}
.logo-class img{
	    width: 3.7em;
    margin-top: 6px;
}
h1 {
    font-size: 1.5em;
    font-weight: normal;
}
.shopping-main {
    margin: 0 0 70px;
}

.class-nav {
	position: fixed;
	left: 0;
	height: 100%;
	width: 80px;
	z-index: 99;
	overflow-y: auto;
	border-right: 1px solid #e6e6e6;
	background: #F3F4F6
}

.class-nav ul {
	margin: 0 0 115px
}

.class-nav li {
	display: block;
	border-bottom: 1px solid #e6e6e6;
	color: #fff;
}

.class-nav li.on {
	background: #fff;
	border-right-color: #fff
}
.class-nav li a {
	display: block;
	line-height: 40px;
	padding: 0 12px;
	color: #474747;
	font-size: 13px;
	font-family: "微软雅黑";
}

.class-nav li a.tinct{
	background-color: red;
}

.class-con {
	position: relative;
	padding-bottom: 10px;
	margin: auto;
}
.search-form .form-control {
	font-size: 12px;
	border: none;
	height: 26px;
	line-height: 26px;
	text-align: center;
	-webkit-box-shadow: none;
	box-shadow: none;
	-webkit-border-radius: 0;
	border-radius: 0
}

.search-form .search-form-btn {
	top: 10px;
	right: 0;
	line-height: 1
}

.class-search-form {
	width: 100%;
	z-index: 2;
	padding: 10px 0;
	background: #fff;
	box-shadow: 0 1px 2px rgba(0,0,0,.1)
}

.class-search-form .form-control {
	font-size: 14px;
	width: 84%;
	height: 35px;
	padding-left: 40px;
	text-align: left;
	background: #eeecec
}

.class-search-form p {
    /*margin: 0 15px 0 105px;*/
	margin:auto;
	margin-left: 11px;
}
.class-search-form .search-form-btn {
	top: 0;
	left: 0;
	color: #999;
	width: 35px;
	height: 35px;
	line-height: 35px;
	background: 0 0;
	text-align: center
}

.class-con ul {
	margin: 0 15px 20px 15px;
	overflow: hidden
}

.class-con li {
	float: left;
	width: 46%;
	text-align: center;
	margin: 0 8% 12px 0
}

.class-con img {
	display: block;
	width: 100%;
	margin: 0 auto 5px
}

.class-con li:nth-child(even) {
	margin-right: 0
}

.class-con a {
	display: block;
	color: #333
}

.class-con h3 {
	font-weight: 400;
    font-size: 13px;
    margin: 0;
    height: 32px;
    line-height: 16px;
    overflow: hidden;
    display: -webkit-box;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    text-align: left;
}

.search-head {
	box-shadow: none;
	border-bottom: 1px solid #cfcfcf
}

.logo-class a {
	display: block;
	width: 90px;
	margin: 0 auto;
	text-indent: -999999em;
	overflow: hidden;
	background: url(/app/resource/images/logo-class.png) no-repeat;
	background-size: 100% auto
}

.head {
	width: 100%;
	background: #e8e8e8;
	overflow: hidden;
	z-index: 99999;
}

.container-fill{
	width: 100%;
	display: block;
	overflow: hidden;
}